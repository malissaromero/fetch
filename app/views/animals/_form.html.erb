<%= form_for @animal do |f| %>

<%= f.label :name %>
<%= f.text_area :name %>

<%= f.label :photo_url %>
<%= f.text_area :photo_url %>

<%= f.label :age %>
<%= f.text_area :age %>

<%= f.label :gender %>
<%= f.text_area :gender %>

<%= f.label :bio %>
<%= f.text_area :bio %>

<%= f.label :variety_id %>
<%= f.references :variety_id %>

<%= f.submit %>

<% end %>
